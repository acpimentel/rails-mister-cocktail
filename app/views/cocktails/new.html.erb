<div>
  <h1>Create a Cocktail</h1>
    <%= simple_form_for @cocktail do |f| %>
      <%= f.input :name %>
      <%= f.association :doses %>
      <%= f.association :ingredients %>
      <%= f.button :submit %>
    <% end %>
<%= link_to "Back to the list", cocktails_path, class: "btn btn-primary" %>
